class Computadora:

    def __init__(self, nombre, monitor, teclado, raton) -> None:
        Computadora.contadorComputadoras += 1
        self._idComputadora = Computadora.contadorComputadoras
        self._nombre = nombre
        self._monitor = monitor
        self._teclado = teclado
        self._raton = raton

    def __str__() -> str:
        return f''